<?xml version="1.0" encoding="utf-8"?>
<View>
	
	<Panel Name="MiniMapPanel" Bounds="Bind MiniMapBounds" InnerMargin="4,4,4,4">
		<Image Name="MiniMap" Alignment="Stretch"/>
	</Panel>
	
	<Panel Name="MapPanel" Anchor="$CenterX,$CenterY" Bounds="-145,-90,290,180">
		<Image Picture="Ref MapRender" Alignment="Stretch" Color="Bind MapColor"/>
	</Panel>
	
	
</View>